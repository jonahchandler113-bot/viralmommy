# Queue Configuration
# Copy this to your .env file and configure as needed

# Redis Configuration (for production)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Use in-memory queue (for development without Redis)
# Set to 'true' to use in-memory queue (no Redis required)
USE_IN_MEMORY_QUEUE=true

# Queue Settings
QUEUE_CONCURRENCY_VIDEO=2
QUEUE_CONCURRENCY_AI=1
QUEUE_CONCURRENCY_STRATEGY=3

# Rate Limiting
CLAUDE_MAX_REQUESTS_PER_MINUTE=10
VIDEO_PROCESSING_MAX_PER_MINUTE=5
